<div class="card bg-dark text-white">
  <img class="card-img" src="https://freerangestock.com/sample/173651/modern-kitchen-with-tablet-recipe-holder.jpg" alt="Card image">
  <div class="card-img-overlay">
    <h4 class="card-title">Cookbook</h4>
    <p class="card-text">See your favourite recipes here!</p>
  </div>
</div>

<div class ="title-top">
<h3> Categories List</h3>
  <%= link_to 'New Category', new_category_path, class: "btn btn-warning" %>
</div>

<div class="cards mt-3">
  <% @categories.each do |category| %>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(data:image/jpeg;base64,)" >
     <div>
       <h2><%= link_to category.name, category_path(category), class:"category-link" %></h2>
       <p><%= category.recipes.count %> recipes</p>
     </div>
    </div>
  <% end %>
</div>

